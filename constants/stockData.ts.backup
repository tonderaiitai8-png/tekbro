// Helper function to initialize stocks with price history
export function initializeStocks(): Stock[] {
    const now = Date.now();
    return STOCK_CATALOG.map(stock => ({
        ...stock,
        history: Array.from({ length: 50 }, (_, i) => ({
            timestamp: now - (49 - i) * 60000, // 1 minute intervals
            value: stock.price * (1 + (Math.random() - 0.5) * 0.02) // ±1% variation
        }))
    }));
}
